#########
## PATH ##
#########
if ! [[ "$PATH" =~ "$HOME/.local/bin:$HOME/bin:" ]]; then
    PATH="$HOME/.local/bin:$HOME/bin:$PATH"
fi
# Look in PWD last.
export PATH=$PATH:.

######################
## Environment Setup ##
######################
if [ -f "$HOME/.rc" ]; then
	. $HOME/.rc
else
	echo "ERROR .rc: Failed to source .rc because it couldn't be found." >&2
fi

#######################
## Lynx Configuration ##
#######################
# Set home page.
export WWW_HOME=https://www.google.com
